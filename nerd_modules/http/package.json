  "version": "0.0.1",
  "nerd":
  {
    "env": ["std"],
    "read_only": [],
    "lib": 
    {
      "cmake_cached": { "name": "uSockets", "path": "__EXTERN__/uSockets" },
      "cmake": ["add_subdirectory(\"__EXTERN__/uSockets\" \"__CACHE__/uSockets\")",
                "target_link_libraries(__PROJ__ uSockets)",
                "target_include_directories(__PROJ__ PRIVATE __EXTERN__/uWebSockets)",
                "target_compile_definitions(__PROJ__ PRIVATE UWS_NO_ZLIB)"]
    }
  }
}
